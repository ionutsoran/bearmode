{
  "multipart": [
    {
      "apply": {
        "model": "bearmode:block/pipe"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "x": -90,
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "bearmode:block/pipe_side",
        "x": 90,
        "uvlock": false
      }
    }
  ]
}